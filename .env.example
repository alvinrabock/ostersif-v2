# SMC API (Swedish Match Center)
SMC_SECRET=your_smc_api_key_here

# eBiljett API
EBILJETT_BASIC_AUTH=your_ebiljett_basic_auth_here

# Frontspace CMS (replaces Payload)
# GraphQL endpoint - can be local or production
FRONTSPACE_ENDPOINT=http://localhost:3000/api/graphql
FRONTSPACE_STORE_ID=your_store_id_here
FRONTSPACE_API_KEY=your_api_key_here

# Frontspace Webhook (optional)
FRONTSPACE_WEBHOOK_URL=your_webhook_url_here
FRONTSPACE_WEBHOOK_SECRET=your_webhook_secret_here

# Legacy Payload CMS (being replaced by Frontspace)
# NEXT_PUBLIC_BACKEND_URL=your_payload_backend_url_here

# Next.js Public Variables (available in browser)
NEXT_PUBLIC_SMC_SECRET=your_smc_api_key_here
