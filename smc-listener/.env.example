# Azure Service Bus connection string from SMC
# Format: Endpoint=sb://p-exp-bmt-sb-smc.servicebus.windows.net/;SharedAccessKeyName=listenreadonly;SharedAccessKey=YOUR_KEY
SERVICE_BUS_CONNECTION_STRING=

# Comma-separated list of Service Bus topics to listen to
SERVICE_BUS_TOPICS=p-sb-smc-sef-allsvenskan,p-sb-smc-sef-superettan

# Subscription name provided by SMC
SERVICE_BUS_SUBSCRIPTION=FRO01-2213135e-18a2-5d2d-3d26-8e8e2a912123

# Next.js webhook URL for cache revalidation
# Example: https://ostersif.se/api/revalidate-match
NEXTJS_WEBHOOK_URL=

# Secret key for authenticating webhook calls (must match SERVICE_BUS_REVALIDATE_SECRET in Next.js env)
SERVICE_BUS_REVALIDATE_SECRET=
